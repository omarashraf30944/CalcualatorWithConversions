<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".WeightsActivity">

    <FrameLayout
        android:id="@+id/frame_layout2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/bottom_navigation2"/>




    <com.google.android.material.bottomnavigation.BottomNavigationView
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/frame_layout2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:tabSelectedTextColor="#D51E1E"
        android:layout_width="match_parent"
        android:id="@+id/bottom_navigation2"
        app:tabIndicatorColor="#0f0"
        android:background="#FFFFFF"
        app:menu="@layout/menu2"
        android:layout_height="wrap_content" />



</androidx.constraintlayout.widget.ConstraintLayout>